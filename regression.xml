<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Allure Test Suite" verbose="1">
	<listeners>
		<listener class-name="io.qameta.allure.testng.AllureTestNg" />
	</listeners>
	<test name="Non Payment First Proposal" preserve-order="true">
		<classes>
			<class name="testcases.NonPayFirstProposal" />
		</classes>
	</test>
	<test name="Payment First Proposal" preserve-order="true">
		<classes>
			<class name="testcases.PayFirstProposal" />
		</classes>
	</test>
	<test name="Feature Compare" preserve-order="true">
		<classes>
			<class name="testcases.FeatureCompare" />
		</classes>
	</test>
	<test name="Travel Partner" preserve-order="true">
		<classes>
			<class name="testcases.TravelPartnerProposal" />
		</classes>
	</test>
</suite>

